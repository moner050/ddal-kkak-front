{
  "name": "ddal-kkak-front",
  "version": "0.1.0",
  "private": true,
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "web": "expo start --web",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "fetch-data": "node scripts/fetch-data.js",
    "test-scheduler": "node scripts/test-scheduler.js",
    "copy-public-data": "node scripts/copy-public-data.js",
    "prebuild:web": "npm run fetch-data",
    "build:web": "expo export --platform web",
    "build:source": "expo export --platform web",
    "postbuild:web": "npm run copy-public-data",
    "dev:local": "cross-env EXPO_PUBLIC_API_URL=http://finance-mhb-api.kro.kr expo start --web",
    "dev:local:android": "cross-env EXPO_PUBLIC_API_URL=http://finance-mhb-api.kro.kr expo run:android",
    "dev:local:ios": "cross-env EXPO_PUBLIC_API_URL=http://finance-mhb-api.kro.kr expo run:ios",
    "serve": "node server.js",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "pm2:restart": "pm2 restart ecosystem.config.js",
    "pm2:logs": "pm2 logs ddal-kkak-front",
    "pm2:monit": "pm2 monit"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
    "axios": "^1.13.2",
    "clsx": "^2.1.1",
    "compression": "^1.7.4",
    "expo": "^54.0.13",
    "expo-constants": "~18.0.9",
    "expo-linking": "~8.0.8",
    "expo-router": "^6.0.12",
    "express": "^4.21.2",
    "http-proxy-middleware": "^3.0.5",
    "morgan": "^1.10.0",
    "node-cron": "^4.2.1",
    "pm2": "^5.4.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-native": "0.81.4",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "^0.21.0",
    "recharts": "^3.5.1",
    "rotating-file-stream": "^3.2.5",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/react": "~19.1.10",
    "autoprefixer": "^10.4.20",
    "babel-preset-expo": "^54.0.0",
    "cross-env": "^10.1.0",
    "postcss": "^8.4.41",
    "tailwindcss": "^3.4.15",
    "typescript": "^5.6.3"
  },
  "expo": {
    "entryPoint": "expo-router/entry"
  }
}
